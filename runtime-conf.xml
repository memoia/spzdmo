<?xml version="1.0" encoding="UTF-8"?>
<config>
  <propel>
    <datasources default="spzdmo">
      <datasource id="spzdmo">
        <adapter>sqlite</adapter>
        <connection>
          <dsn>sqlite:what.db</dsn>
        </connection>
      </datasource>
    </datasources>
    <log>
      <logger name="defaultLogger">
        <type>stream</type>
        <path>propel.log</path>
        <level>300</level>
      </logger>
    </log>
  </propel>
</config>
